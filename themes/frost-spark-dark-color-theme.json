			"settings": {
				"foreground": "#f0f0f0",
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
			],
			"settings": {
				"foreground": "#a2d2a8",
				"fontStyle": "italic"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"entity.name.import",
			],
			"settings": {
				"foreground": "#ffa904"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#bfe57b"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant",
			],
			"settings": {
				"foreground": "#f2c090"
			}
		},
		{
			"name": "Entity",
			"scope": [
				"entity",
			],
			"settings": {
				"foreground": "#ff6a38"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
				"keyword.operator.star.sql",
