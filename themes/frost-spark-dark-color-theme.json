				// "variable.other.assignment",
				// "variable.other.property",
			],
			"settings": {
				"foreground": "#9bcbec"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"storage",
				// "keyword.control",
				// "keyword.other",
				"variable.other.property",
			],
			"settings": {
				"foreground": "#edb0ff"
			}
		},
